<%- include('../partials/header.ejs'); %>

<div class="container">
    <form action="/nodes" method="POST">
        <fieldset>
            <legend>Add a node</legend>

            <div class="form-group">
                <label>Node Name</label>
                <input type="text" class="form-control" name="name"
                    placeholder="Enter Camera Name"
                    style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;">
                <small id="emailHelp" class="form-text text-muted">Camera name could be any. example: Camera1</small>
            </div>
            <div class="form-group">
                <label>IP address</label>
                <input type="text" class="form-control" name="ip"
                    placeholder="Enter node ip address"
                    style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;">
                <small id="emailHelp" class="form-text text-muted">Example 192.168.88.5</small>
            </div>



            <button type="submit" class="btn btn-primary">Submit</button>
        
    </form>

</div>


<%- include('../partials/footer.ejs'); %>